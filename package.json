{
  "name": "sandbox",
  "version": "1.1.0",
  "description": "jyj sandbox",
  "keywords": [
    "jyj"
  ],
  "author": "jyj",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "doc": "npx typedoc --options ./typedocconfig.ts",
    "exec": "ts-node tests/tests/test.ts",
    "generate": "ts-node ./tools/generate_interface.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jyj-upminder/sandbox.git"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "ajv-keywords": "^3.4.1"
  },
  "devDependencies": {
    "@types/mongodb": "^3.1.32",
    "mongodb": "^3.3.0-beta2",
    "@types/chance": "^1.0.6",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.0",
    "eslint": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.8.0",
    "json-schema-to-typescript": "^7.1.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0"
  }
}
